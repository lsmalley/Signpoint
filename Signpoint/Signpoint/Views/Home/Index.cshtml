@{
    ViewBag.Title = "Home Page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!--indicators-->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="2" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="3" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="4" class="active"></li>
    </ol>

    <!--wrapper-->
    <div class="carousel-inner">
        <div class="item active">
            <img src="~/Content/stitch.jpg" alt="stitch"/>
        </div>

        <div class="item">
            <img src="~/Content/eeyore.jpg"  alt="eeyore"/>
        </div>

        <div class="item">
            <img src="~/Content/sadness.jpg" alt="sadness"/>
        </div>

        <div class="item">
            <img src="~/Content/sully.jpg" alt="sully"/>
        </div>

        <div class="item">
            <img src="~/Content/dory.jpg" alt="dory"/>
        </div>
    </div>

        <!--Left control-->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>

        <!--Right control-->
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
</div>